/**index.wxss**/
page {
  background-color: #f4f4f4;
}
.container {
  position: relative;
  width: 100%;
  height: 100%;
  background-color: #f4f4f4;
  color: #939393;
}

.card {
  border: 2px solid #ffffff;
  border-radius: 5px;
  background-color: #ffffff;
  box-shadow: 0px 5px 1px #cccccc;
  margin: 7px;
  position: relative;
}
.text {
  padding: 4px;
  font-size: 15px;
  color: #000000;
  text-align: center;
}
.image{
  width: 100%;
  height: 120px;
}