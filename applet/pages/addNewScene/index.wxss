/**index.wxss**/
page {
  background-color: #f4f4f4;
}

.name {
  background-color: #ffffff;
  font-size: 12pt;
  margin-top: 5px;
  margin-bottom: 5px;
  padding: 10px;
}

.container {
  position: relative;
  width: 100%;
  height: 100%;
  top: 10px;
  bottom: 10px;
}

.choose {
  position: relative;
  font-size: 10pt;
  color: #acacac;
  top: 5px;
  left: 5px;
}

.row {
    display: flex;
    flex-direction: row;
    flex: 1;
    justify-content: space-around;
    flex-wrap: wrap;
}

.item_size {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    height: 250rpx;
    width: 250rpx;
}

.img {
    width: 230rpx;
    height: 230rpx;
}