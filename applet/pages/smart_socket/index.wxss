page {
  background-color: #11EE96;
}

.part_1 {
  width: 100%;
  height: 700rpx;
  text-align: center;
}

.circle {
  position: absolute;
  top: 200rpx;
  left: 0;
  right: 0;
  width: 300rpx;
  height: 300rpx;
  border-radius: 50%;
}

.text {
  position: absolute;
  top: 520rpx;
  left: 0;
  right: 0;
  color: white;
  font-size: 33rpx;
}

.text1 {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: left;
  position: absolute;
  left: 5%;
  bottom: 30%;
}

.text2 {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: left;
  position: absolute;
  left: 40%;
  bottom: 30%;
}

.text3 {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: left;
  position: absolute;
  right: 5%;
  bottom: 30%;
}

.text_1 {
  position: relative;
  font-size: 32rpx;
  color: #ffffff;
}
.text_2 {
  position: relative;
  font-size: 32rpx;
  color: #ffffff;
}

.part_2 {
  position: absolute;
  bottom: 0;
  width: 100%;
  height: 280rpx;
  background-color: #ffffff;
}

.row {
  display: flex;
  flex-direction: row;
  flex: 1;
  justify-content: space-around;
  flex-wrap: wrap;
}

.choice {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  height: auto;
  width: 250rpx;
}

.button {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  margin-top: 50rpx;
}

.button::after {
  border: none;
}

.texts {
  font-size: 10pt;
  margin-top: 15rpx;
  margin-bottom: 15rpx;
  color: #5a5a5a;
}